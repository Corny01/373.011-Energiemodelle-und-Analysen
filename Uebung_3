    "# 3.1 d) Stündlicher Strompreis\n",
    "\n",
    "for t in timesteps:\n",
    "    maxMC=0\n",
    "    for n in thermalPlant:\n",
    "        if model.x[n,t].value >0:\n",
    "            if MC[n] > maxMC:\n",
    "                maxMC = MC[n]\n",
    "    print('Stromkosten in Stunde', t, ':', np.round(maxMC,2), '€/MWh')"
   ]
