    "\n",
    "print(\"Werte der Schattenvariable bezüglich Lastbeschränkung\")\n",
    "dual_values = []\n",
    "for t in timesteps:\n",
    "    value = model.dual[model.load_con[t]]\n",
    "    dual_values.append(value)\n",
    "    print(\"Stunde\", t, \":\", np.round(dual_values[t],2), \"€/MWh\")"
